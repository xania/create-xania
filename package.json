  "files": [
    "dist",
    "templates"
  ],
  "devDependencies": {
    "rimraf": "^3.0.2",
    "@types/node": "^17.0.29",
    "tsup": "^5.12.1",
    "typescript": "^4.6.3",
    "prettier": "^2.8.1"
  },
  "license": "MIT",
  "dependencies": {
    "create-create-app": "^7.3.0"
  }
